let ourMember = obj => list =>
    ((list === [])?
     (nill) :
     ((list[0] === obj)?
      (list[0]):
      (ourMember(obj)(list.slice(1)))));

let cdr = list => list.slice(1);

var max = (numeros, maior = numeros[0]) =>
  numeros.length === 0
    ? maior
    : numeros[1] > maior
    ? max(numeros.slice(1), numeros[1])
    : max(numeros.slice(1), maior);
